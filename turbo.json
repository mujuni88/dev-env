{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "setup": {
      "dependsOn": ["system-install", "global-install", "dotfiles-install"],
      "cache": false,
      "outputs": []
    },
    "system-install": {
      "cache": false,
      "outputs": []
    },
    "global-install": {
      "dependsOn": ["^system-install"],
      "cache": false,
      "outputs": []
    },
    "dotfiles-install": {
      "dependsOn": ["^system-install", "^global-install"],
      "cache": false,
      "outputs": []
    },
    "build": {
      "cache": false
    }
  }
}
