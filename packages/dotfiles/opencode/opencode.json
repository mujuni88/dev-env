{
  "$schema": "https://opencode.ai/config.json",
  "permission": "allow",
  "plugin": ["opencode-antigravity-auth@beta"],
  "instructions": [
    "~/.claude/CLAUDE.md",
    "CLAUDE.md",
    "CLAUDE.local.md",
    ".cursor/rules/*.md"
  ],
  "provider": {
    "anthropic": {
      "options": {
        "baseURL": "http://localhost:9123/mtlsproxy:claudecode/v1"
      }
    },
    "google": {
      "models": {
        "antigravity-gemini-3-pro": {
          "name": "Gemini 3 Pro (Antigravity)",
          "limit": { "context": 1048576, "output": 65535 },
          "modalities": { "input": ["text", "image", "pdf"], "output": ["text"] },
          "variants": {
            "low": { "thinkingLevel": "low" },
            "high": { "thinkingLevel": "high" }
          }
        },
        "antigravity-gemini-3-flash": {
          "name": "Gemini 3 Flash (Antigravity)",
          "limit": { "context": 1048576, "output": 65536 },
          "modalities": { "input": ["text", "image", "pdf"], "output": ["text"] },
          "variants": {
            "minimal": { "thinkingLevel": "minimal" },
            "low": { "thinkingLevel": "low" },
            "medium": { "thinkingLevel": "medium" },
            "high": { "thinkingLevel": "high" }
          }
        },
        "antigravity-claude-sonnet-4-5": {
          "name": "Claude Sonnet 4.5 (Antigravity)",
          "limit": { "context": 200000, "output": 64000 },
          "modalities": { "input": ["text", "image", "pdf"], "output": ["text"] }
        },
        "antigravity-claude-sonnet-4-5-thinking": {
          "name": "Claude Sonnet 4.5 Thinking (Antigravity)",
          "limit": { "context": 200000, "output": 64000 },
          "modalities": { "input": ["text", "image", "pdf"], "output": ["text"] },
          "variants": {
            "low": { "thinkingConfig": { "thinkingBudget": 8192 } },
            "max": { "thinkingConfig": { "thinkingBudget": 32768 } }
          }
        },
        "antigravity-claude-opus-4-5-thinking": {
          "name": "Claude Opus 4.5 Thinking (Antigravity)",
          "limit": { "context": 200000, "output": 64000 },
          "modalities": { "input": ["text", "image", "pdf"], "output": ["text"] },
          "variants": {
            "low": { "thinkingConfig": { "thinkingBudget": 8192 } },
            "max": { "thinkingConfig": { "thinkingBudget": 32768 } }
          }
        },
        "gemini-2.5-flash": {
          "name": "Gemini 2.5 Flash (Gemini CLI)",
          "limit": { "context": 1048576, "output": 65536 },
          "modalities": { "input": ["text", "image", "pdf"], "output": ["text"] }
        },
        "gemini-2.5-pro": {
          "name": "Gemini 2.5 Pro (Gemini CLI)",
          "limit": { "context": 1048576, "output": 65536 },
          "modalities": { "input": ["text", "image", "pdf"], "output": ["text"] }
        },
        "gemini-3-flash-preview": {
          "name": "Gemini 3 Flash Preview (Gemini CLI)",
          "limit": { "context": 1048576, "output": 65536 },
          "modalities": { "input": ["text", "image", "pdf"], "output": ["text"] }
        },
        "gemini-3-pro-preview": {
          "name": "Gemini 3 Pro Preview (Gemini CLI)",
          "limit": { "context": 1048576, "output": 65535 },
          "modalities": { "input": ["text", "image", "pdf"], "output": ["text"] }
        }
      }
    }
  },
  "mcp": {
    "context7": {
      "type": "remote",
      "url": "https://mcp.context7.com/mcp",
      "headers": {
        "CONTEXT7_API_KEY": "$CONTEXT7_API_KEY"
      },
      "enabled": true
    },
    "chrome-devtools": {
      "type": "local",
      "command": ["npx", "chrome-devtools-mcp@latest"],
      "enabled": true,
      "environment": {}
    },
    "hawkins-scraper": {
      "type": "local",
      "command": ["npx", "-y", "@netflix-internal/mcp-server-hawkins-scraper"],
      "enabled": true,
      "environment": {
        "HAWKINS_ASSETS_PATH": "/Users/jbuza/Code/Netflix/netflix-libs/node_modules/@hawkins/assets/",
        "HAWKINS_COMPONENTS_PATH": "/Users/jbuza/Code/Netflix/netflix-libs/node_modules/@hawkins/components",
        "HAWKINS_FORMS_PATH": "/Users/jbuza/Code/Netflix/netflix-libs/node_modules/@hawkins/forms",
        "HAWKINS_HELPERS_PATH": "/Users/jbuza/Code/Netflix/netflix-libs/node_modules/@hawkins/helpers/",
        "HAWKINS_ICONS_PATH": "/Users/jbuza/Code/Netflix/netflix-libs/node_modules/@hawkins/icons/",
        "HAWKINS_SEARCH_PATH": "/Users/jbuza/Code/Netflix/netflix-libs/node_modules/@hawkins/search/",
        "HAWKINS_STYLES_PATH": "/Users/jbuza/Code/Netflix/netflix-libs/node_modules/@hawkins/styles/",
        "HAWKINS_TABLE_PATH": "/Users/jbuza/Code/Netflix/netflix-libs/node_modules/@hawkins/table",
        "HAWKINS_VARIABLES_PATH": "/Users/jbuza/Code/Netflix/netflix-libs/node_modules/@hawkins/variables/"
      }
    },
    "genai-mcp-gateway": {
      "type": "local",
      "command": [
        "sh",
        "-c",
        "[ -x /apps/nflx-genai-tool-registry/bin/nflx-genai-tool-registry ] && exec /apps/nflx-genai-tool-registry/bin/nflx-genai-tool-registry || exec newt --app-type=python uvx --tool=python=3.12 -- nflx-genai-tool-registry@latest"
      ],
      "enabled": true,
      "environment": {
        "REGISTRY_ENV": "prod",
        "REGISTRY_TOOLBOX_ID": "core_tools",
        "REGISTRY_URL_OVERRIDE": "sbn-dev-agent"
      }
    },
    "netflix-eng": {
      "type": "remote",
      "url": "http://localhost:9123/mcp",
      "enabled": true
    }
  }
}
